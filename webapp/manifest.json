{
	"_version": "1.7.0",
	"sap.app": {
		"id": "COM.QMPORTALQMPORTAL",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZODATA_QM_INTERNPROJECT_SRV": {
				"uri": "/sap/opu/odata/sap/ZODATA_QM_INTERNPROJECT_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "COM.QMPORTALQMPORTAL.view.ROOTVIEW",
			"type": "XML"
		},
		"handleValidation": false,
		"config": {},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "COM.QMPORTALQMPORTAL.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true,
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "digital",
					"pattern": "",
					"target": "TargetDigital"
				},
				{
					"name": "portfolio",
					"pattern": "portfolio",
					"target": "TargetPortfolio"
				},
				{
					"name": "login",
					"pattern": "login",
					"target": "TargetLogin"
				},
				{
					"name": "dashboard",
					"pattern": "dashboard/{value}",
					"target": "TargetDashboard"
				},
				{
					"name": "home",
					"pattern": "home/{value}",
					"target": "TargetHome"
				}
			],
			"targets": {
				"TargetDigital": {
					"viewId": "digital",
					"viewName": "DIGITAL",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"TargetPortfolio": {
					"viewId": "landing",
					"viewName": "PORTFOLIO",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"TargetLogin": {
					"viewId": "login",
					"viewName": "LOGINPAGE",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"TargetDashboard": {
					"viewId": "dashboard",
					"viewName": "DASHBOARD",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				},
				"TargetHome": {
					"viewId": "home",
					"viewName": "HOME",
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "COM.QMPORTALQMPORTAL.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}